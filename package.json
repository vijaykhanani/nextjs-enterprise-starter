{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "npx gitmoji -c",
    "lint": "next lint --no-cache",
    "prepare": "husky install",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "security-audit": "npm audit",
    "security-audit:json": "npm audit --json",
    "security-audit:prod": "npm audit --production --only=prod --audit-level=low",
    "security-audit:dev": "npm audit --only=dev --audit-level=moderate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "jest --collect-coverage",
    "open:coverage": "start ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "axios": "^0.21.4",
    "i18next": "^20.6.1",
    "i18next-http-backend": "^1.3.1",
    "logatim": "^0.9.0",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/react": "17.0.21",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "autoprefixer": "^10.3.4",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "commitlint": "^13.1.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "enzyme": "^3.11.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.2",
    "gitmoji-cli": "^4.6.1",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^2.2.15",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.4.3",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix --max-warnings 0"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
